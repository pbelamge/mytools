#!/bin/bash

export KUBE_CONFIG_DIR="$HOME/.kube"
export KUBE_JOIN_PARAM=""
export KUBE_POD_CIDR="192.168.0.0/16"
export KUBE_SVC_CIDR="10.96.0.0/16"
