export NODE_IP="172.16.100.20"
export NODE_MASK="255.255.255.0"
export NODE_DOM="kmaster.com"
export NODE_NAME="kmaster"
export PROXY_ENABLED="true"

export http_proxy=""
export https_proxy=""
export no_proxy=""
export HTTP_PROXY=""
export HTTPS_PROXY=""
